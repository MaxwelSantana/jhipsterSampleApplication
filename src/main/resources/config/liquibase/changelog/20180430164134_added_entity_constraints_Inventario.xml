<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Inventario.
    -->
    <changeSet id="20180430164134-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="funcionario_lider_id"
                                 baseTableName="inventario"
                                 constraintName="fk_inventario_funcionario_lider_id"
                                 referencedColumnNames="id"
                                 referencedTableName="funcionario"/>

        <addForeignKeyConstraint baseColumnNames="cliente_id"
                                 baseTableName="inventario"
                                 constraintName="fk_inventario_cliente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente"/>

    </changeSet>
</databaseChangeLog>
