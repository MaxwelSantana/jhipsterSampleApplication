relationship OneToMany {
	Cliente{inventarios} to Inventario
}

relationship OneToOne {
	Inventario{funcionarioLider} to Funcionario
    
    Transmissao{inventario} to Inventario
    Transmissao{funcionario} to Funcionario
    Transmissao{coletor} to Coletor
    Transmissao{tipoContagem} to TipoContagem
    
    Secao{transmissao} to Transmissao
    Secao{statusContagem} to StatusContagem
    
    Contagem{secao} to Secao
    Contagem{statusContagem} to StatusContagem
    Contagem{motivoAlteracao} to MotivoAlteracao
    
    LogAlteracaoContagem{contagem} to Contagem
    
    Cadastro{inventario} to Inventario
    
    Mapeamento{inventario} to Inventario
    
    LogStatusInventario{statusInventario} to StatusInventario
    LogStatusInventario{inventario} to Inventario
    
    CampoCustomizavelColeta{cliente} to Cliente
    
    CampoCustomizavelCadastro{cliente} to Cliente
    
    Configuracao{cliente} to Cliente
    Configuracao{tipoConfiguracao} to TipoConfiguracao
    Configuracao{codigoCustomizado} to CodigoCustomizavel
    
    ConfiguracaoColetor{cliente} to Cliente
    ConfiguracaoColetor{codigoCustomizado} to CodigoCustomizavel
}




